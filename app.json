{
  "expo": {
    "name": "MobileBurner",
    "slug": "MobileBurnerApp",
    "description": "A mobile MQTT controller for the Afterburner Chinese Diesel Heater controller.",
    "platforms": [
      "ios",
      "android"
    ],
    "version": "1.0.1",
    "orientation": "portrait",
    "icon": "./assets/icon.png",
    "splash": {
      "image": "./assets/splash.png",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "updates": {
      "fallbackToCacheTimeout": 0
    },
    "assetBundlePatterns": [
      "**/*"
    ],
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.damianhodgkiss.MobileBurnerApp",
      "buildNumber": "1.0.1"
    },
    "android": {
      "package": "com.damianhodgkiss.MobileBurnerApp"
    }
  }
}
